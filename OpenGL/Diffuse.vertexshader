#version 330 // OpenGL version 3.3 (minimum requirement)

in vec3 vertices; 
in vec3 normals; // Normals
in vec2 texCoords;

uniform mat4 WVP;

out vec3 retNormal; 
out vec2 retTexCoord;

void main()
{
	gl_Position = WVP * vec4(vertices, 1.0); // Transform the model to screen projection
	retNormal = normals;
	retTexCoord = texCoords;
}